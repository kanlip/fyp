@using FYP.Models


@{
    Staff curStaff = Context.Session.GetObject<Staff>("staff");
   
}
@{
    ViewBag.Title = "Customers Report Count";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var lst = ViewData["reports"] as List<dynamic>;
}
<style>
    h2 {
        padding: 100px 25px;
    }
</style>
<h2 style="text-align:center">@curStaff.Name's Customers Report Count</h2>



<table class="table table-bordered table-condensed table-hover table-striped" style="width:70%;margin:auto">
    <tr>

        <th>Customer ID</th>
        <th>Customer Name</th>
        <th>Appointment count</th>
    </tr>

    @foreach (var item in lst)
    {
        <tr>
            <td>@item.cus_id</td>
            <td>@item.Name</td>
            <td>@item.Count</td>
        </tr>
    }
</table>

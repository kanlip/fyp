@using FYP.Models



@{
    ViewBag.Title = "Monthly Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var lst = ViewData["Mreports"] as List<dynamic>;
}
<style>
    h2 {
        padding: 100px 25px;
    }

   
</style>
<h2 style="text-align:center">Customers Monthly Report</h2>



<table class="table table-bordered table-condensed table-hover table-striped" style="width:70%;margin:auto">
    <tr>

        <th>Customer ID</th>
        <th>Customer Name</th>
        <th>Appointment count</th>
    </tr>
    @foreach (var item in lst)
    {
        <tr>
            <td>@item.cus_id</td>
            <td>@item.Name</td>
            <td>@item.Count</td>
        </tr>
    }
</table>
